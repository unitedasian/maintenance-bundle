{% extends "UAMDatatablesBundle:Datatables:list.json.twig" %}

{% block data %}
{% set uam_maintenance = entity %}
{
	"DT_RowId": "{{ 'uam_maintenance' ~ uam_maintenance.id }}",
	"DT_RowClass": "{{ 'uam_maintenance' }}",
	"date_start": "{{ uam_maintenance.dateStart|datetime('medium', 'short', null, app.request.locale) }}",
	"date_end": "{{ uam_maintenance.dateEnd|datetime('medium', 'short', null, app.request.locale) }}",
	"description": "{{ uam_maintenance.setLocale(app.request.locale).description }}",
	"confirmed": "{{ uam_maintenance.confirmed }}",
	"urls": {
		"edit": "{{ path('uam_maintenance_admin_edit', { id: uam_maintenance.id }) }}",
		"delete": "{{ path('uam_maintenance_admin_delete', { id: uam_maintenance.id}) }}"
	}
}
{% endblock %}
